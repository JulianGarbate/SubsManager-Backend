{
  "version": 2,
  "buildCommand": "npm run build && npx prisma generate",
  "outputDirectory": "dist",
  "framework": "express",
  "env": {
    "DATABASE_URL": "@database_url",
    "JWT_SECRET": "@jwt_secret",
    "PORT": "4000"
  },
  "functions": {
    "src/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
